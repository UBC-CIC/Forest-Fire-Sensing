  "paths": {
    "/cancelSub/{username}/{createTime}": {
      "name": "/cancelSub/{username}/{createTime}",
      "lambdaFunction": "deleteUserSub",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/sub/{username}/{coord}": {
      "name": "/sub/{username}/{coord}",
      "lambdaFunction": "addUserSub",
