        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/user-sensors": {
      "name": "/user-sensors",
      "lambdaFunction": "getUserSensors",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/location/{sensorID}": {
      "name": "/location/{sensorID}",
      "lambdaFunction": "491putLocation",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
